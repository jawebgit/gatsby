{
  "name": "gatsby-typegen-remark",
  "version": "0.0.0",
  "description": "Gatsby plugin which generates graphql types based on Markdown files using the remark markdown ecosystem",
  "main": "index.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir .",
    "watch": "babel -w src --presets babel-preset-es2015 --out-dir ."
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "remark"
  ],
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "excerpt-html": "^1.2.0",
    "graphql": "^0.8.2",
    "lodash": "^4.17.2",
    "remark": "^6.2.0",
    "remark-html": "^5.0.1",
    "sanitize-html": "^1.13.0",
    "unist-util-select": "^1.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0"
  }
}
