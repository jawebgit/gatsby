    "hast-util-to-html": "^3.0.0",
    "is-relative-url": "^2.0.0",
    "lodash": "^4.17.4",
    "mdast-util-to-hast": "^2.1.1",
    "prismjs": "^1.6.0",
    "remark": "^6.2.0",
    "remark-html": "^5.0.1",
    "sanitize-html": "^1.13.0",
    "underscore.string": "^3.3.4",
    "unist-util-inspect": "^4.0.0",
