    "cross-env": "^5.0.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.38",
    "nprogress": "^0.2.0"
  },
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  }
}
